{
  "name": "snips-action-sports",
  "version": "1.0.0",
  "description": "Snips action code for the Sports app",
  "main": "dist/index.js",
  "scripts": {
    "start": "npm run lint && npm run build && npm run test",
    "build": "snips-toolkit build",
    "dev": "snips-toolkit dev",
    "test": "snips-toolkit test '^.*\\.spec\\.[jt]s$'",
    "test:sandbox": "snips-toolkit test -s '^.*\\.spec\\.[jt]s$'",
    "launch": "snips-toolkit run",
    "lint": "eslint \"**/*.[jt]s\""
  },
  "author": "Thomas Bouvier <contact@thomas-bouvier.io>",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^24.0.15",
    "@types/node": "^12.0.8",
    "@types/node-cron": "^2.0.2",
    "@typescript-eslint/eslint-plugin": "^1.10.2",
    "@typescript-eslint/parser": "^1.10.2",
    "eslint": "^5.16.0",
    "eslint-plugin-jest": "^22.7.0",
    "hermes-javascript": "^0.3.12",
    "typescript": "^3.5.2"
  },
  "dependencies": {
    "moment": "^2.24.0",
    "node-cron": "^2.0.3",
    "snips-toolkit": "^1.0.1"
  },
  "sandbox": [
    "assert",
    "buffer",
    "events",
    "fs",
    "http",
    "https",
    "net",
    "path",
    "stream",
    "tty",
    "url",
    "util",
    "zlib"
  ]
}
